pipeline {
    agent any
    stages {
        stage('Scm Checkout'){
            steps {
                git 'https://github.com/dummyrepos/spring-petclinic.git' 
            }
        }
        stage('Package'){
            steps {
                sh 'mvn package'
            }
        }
         stage('Docker Build') {
      agent any
      steps {
        sh 'docker build -t shivadeshapathi/spring-petclinic:latest .'
      }
    }
   }
}
 
